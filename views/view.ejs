<%- include("header") -%>

    <h1 class="box" id="heading"><%= dateObj %></h1>

    <div class="box">
      <% for (var i = 0; i < items.length; i++) { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= items[i] %></p>
      </div>

      <% } %>

      <form action="/" method="POST">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off"/>
        <button type="submit" name="button">+</button>
      </form>
    </div>

<%- include("footer") %>
